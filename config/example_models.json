      "enabled": true,
      "max_tokens": 2000,
      "temperature": 0.6,
      "timeout": 30,
      "retry_attempts": 3,
      "rate_limit": {
        "requests_per_minute": 100,
        "tokens_per_minute": 200000
      },
      "cost": {
        "input_token_cost": 0.0014,
        "output_token_cost": 0.0028
      }
    },
    "deepseek-coder": {
      "provider": "deepseek",
      "model_name": "deepseek-coder",
      "base_url": "https://api.deepseek.com/v1",
      "api_key": "${DEEPSEEK_API_KEY}",
      "enabled": true,
      "max_tokens": 4000,
      "temperature": 0.3,
      "timeout": 45,
      "retry_attempts": 3,
      "rate_limit": {
        "requests_per_minute": 50,
        "tokens_per_minute": 100000
      },
      "cost": {
        "input_token_cost": 0.0014,
        "output_token_cost": 0.0028
      }
    },
    "glm-4": {
      "provider": "zhipu",
      "model_name": "glm-4",
      "base_url": "https://open.bigmodel.cn/api/paas/v4",
      "api_key": "${GLM_API_KEY}",
