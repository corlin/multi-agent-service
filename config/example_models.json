      "cost": {
        "input_token_cost": 0.004,
        "output_token_cost": 0.012
      }
    },
    "deepseek-chat": {
      "provider": "deepseek",
      "model_name": "deepseek-chat",
      "base_url": "https://api.deepseek.com/v1",
