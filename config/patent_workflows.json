{
  "workflows": {
    "patent_comprehensive_analysis": {
      "workflow_id": "patent_comprehensive_analysis",
      "name": "专利综合分析工作流",
      "description": "完整的专利分析流程，包括数据收集、搜索增强、分析处理和报告生成",
      "workflow_type": "sequential",
      "enabled": true,
      "participating_agents": [
        "patent_data_collection_agent",
        "patent_search_agent",
        "patent_analysis_agent",
        "patent_report_agent"
      ],
      "execution_order": [
        "patent_data_collection_agent",
        "patent_search_agent",
        "patent_analysis_agent",
        "patent_report_agent"
      ],
      "timeout": 1800,
      "retry_policy": {
        "max_retries": 2,
        "retry_delay": 10,
        "backoff_factor": 1.5
      },
      "metadata": {
        "category": "patent_analysis",
        "priority": "high",
        "estimated_duration": 1200,
        "steps": [
          {
            "step_id": "data_collection",
            "name": "专利数据收集",
            "agent_type": "patent_data_collection",
            "required": true,
            "timeout": 300,
            "retry_count": 3,
            "dependencies": [],
            "parameters": {
              "max_patents": 1000,
              "data_sources": [
                "google_patents",
                "patent_public_api"
              ],
              "enable_cache": true
            }
          },
          {
            "step_id": "search_enhancement",
            "name": "搜索增强",
            "agent_type": "patent_search",
            "required": false,
            "timeout": 180,
            "retry_count": 2,
            "dependencies": [],
            "parallel_with": [
              "data_collection"
            ],
            "parameters": {
              "enable_cnki": true,
              "enable_bocha": true,
              "enable_crawling": true
            }
          },
          {
            "step_id": "analysis",
            "name": "专利分析",
            "agent_type": "patent_analysis",
            "required": true,
            "timeout": 600,
            "retry_count": 2,
            "dependencies": [
              "data_collection"
            ],
            "parameters": {
              "analysis_types": [
                "comprehensive"
              ],
              "min_confidence": 0.7
            }
          },
          {
            "step_id": "report_generation",
            "name": "报告生成",
            "agent_type": "patent_report",
            "required": false,
            "timeout": 300,
            "retry_count": 2,
            "dependencies": [
              "analysis"
            ],
            "parameters": {
              "format": "html",
              "template": "comprehensive"
            }
          }
        ],
        "success_criteria": {
          "min_steps_completed": 2,
          "required_steps": [
            "data_collection",
            "analysis"
          ],
          "min_quality_score": 0.6
        }
      }
    },
